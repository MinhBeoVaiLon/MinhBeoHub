    loadstring(game:HttpGet(('https://raw.githubusercontent.com/AltsegoD/scripts/egoD/tablekhoc.lua'),true))()
end)

RobloxSection:NewButton("ZenHub", "ZenHub", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/Kaizenofficiall/ZenHub/main/Loader'),true))()
end)

RobloxSection:NewButton("MukuroHub", "MukuroHub", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/xDepressionx/Free-Script/main/AllScript.lua'),true))()
end)

